{
  "name": "单词发音释意-搜索",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "search-pronounce-chromium-extension",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },

  "permissions": ["tabs", "contextMenus"],
  "web_accessible_resources": [
    {
      "resources": ["*.js", "*.css", "*.html"],
      "matches": ["*://*/*"],
      "extension_ids": [],
      "use_dynamic_url": true
    }
  ],
  "author": "https://github.com/jingjingxyk",
  "homepage_url": "https://github.com/jingjingxyk/search-pronounce-extension.git"
}
